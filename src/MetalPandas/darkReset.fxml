<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import java.net.URL?>
<AnchorPane fx:id="resetBackground" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MetalPandas.DarkReset">
   <Button fx:id="confirmButton" layoutX="307.0" layoutY="218.0" mnemonicParsing="false" onAction="#handleConfirmAction" text="Confirm" />
   <PasswordField fx:id="password" layoutX="217.0" layoutY="133.0" promptText="New Password" />
   <PasswordField fx:id="reenterPassword" layoutX="217.0" layoutY="175.0" promptText="Reenter New Password" />
   <Text fx:id="resetPasswordLabel" layoutX="133.0" layoutY="63.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Reset Password:">
      <font>
         <Font size="45.0" />
      </font>
   </Text>
   <stylesheets>
      <URL value="@darkTheme.css" />
   </stylesheets>
</AnchorPane>
